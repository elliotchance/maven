repchar('-', 70);
System.println("Data type   Range");
repchar('=', 70);
System.println("char        0 - 65,535");
System.println("short       -32,768 - 32,767");
System.println("int         -2,147,483,648 - 2,147,483,647");
System.println("long        -9,223,372,036,854,775,808 - 9,223,372,036,854,775,807");

void repchar(char ch, int n) {
	for(int j = 0; j < n; j++)
		System.print(ch);
	System.println();
}
