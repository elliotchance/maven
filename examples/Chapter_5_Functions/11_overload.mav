repchar();
repchar('=');
repchar('+', 30);

void repchar() {
	for(int j = 0; j < 45; j++)
		System.print('*');
	System.println();
}

void repchar(char ch) {
	for(int j = 0; j < 45; j++)
		System.print(ch);
	System.println();
}

void repchar(char ch, int n) {
	for(int j = 0; j < n; j++)
		System.print(ch);
	System.println();
}
